<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="72.0" prefWidth="333.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.TagTemplateCtrl">
   <children>
      <CheckBox fx:id="checkbox" layoutX="1.0" layoutY="31.0" mnemonicParsing="false" onAction="#setCheckbox" prefHeight="17.0" prefWidth="22.0" />
      <Button fx:id="deleteButton" layoutX="282.0" layoutY="23.0" mnemonicParsing="false" onAction="#deleteTag" prefHeight="17.0" prefWidth="22.0" text="x" />
      <TextField fx:id="title" layoutX="19.0" layoutY="27.0" onAction="#setTitle" prefHeight="17.0" prefWidth="121.0" text="New Tag" />
      <ColorPicker fx:id="font" layoutX="148.0" layoutY="23.0" onAction="#setFont" prefHeight="25.0" prefWidth="53.0" promptText="tytrt" />
      <ColorPicker fx:id="highlight" layoutX="202.0" layoutY="23.0" onAction="#setHighlight" prefHeight="25.0" prefWidth="53.0" />
   </children>
</AnchorPane>
